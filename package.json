{
  "name": "ga-tracker-snippet",
  "version": "1.0.0",
  "description": "Generate a code snippet to create a Google Analytics tracker object",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "pretest": "bower install --production",
    "test": "gulp test",
    "test-only": "tape test-*.js",
    "coverage": "istanbul cover -x=bower_components/** test-api.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "repository": "shinnn/ga-tracker-snippet",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/ga-tracker-snippet/blob/master/LICENSE"
    }
  ],
  "keywords": [
    "ga",
    "tracker",
    "code",
    "script",
    "google",
    "analytics",
    "google analytics",
    "universal analytics",
    "snippet",
    "generate",
    "template",
    "customize",
    "cli",
    "command-line",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "is-var-name": "^1.0.0",
    "minimist": "^1.1.1",
    "sum-up": "^1.0.1"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "browserify": "^13.1.0",
    "each-exec": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-jscs": "^4.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-load-plugins": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "istanbul": "^0.4.5",
    "istanbul-coveralls": "^1.0.3",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "merge-stream": "^1.0.0",
    "require-bower-files": "^2.0.0",
    "rimraf": "^2.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "tiny-npm-license": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98
  }
}
