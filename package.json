    "sum-up": "^1.0.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "browserify": "^6.3.3",
    "each-exec": "^1.0.0",
    "exec-series": "^1.0.1",
    "gulp": "^3.8.10",
    "gulp-jscs": "^1.3.1",
    "gulp-jshint": "^1.9.0",
    "gulp-jsonlint": "^1.0.1",
    "gulp-load-plugins": "^0.7.1",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.0",
    "gulp-wrap": "^0.5.0",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jshint-stylish": "^1.0.0",
    "merge-stream": "^0.1.6",
    "require-bower-files": "^1.0.0",
    "rimraf": "^2.2.8",
    "tape": "^3.0.3",
    "tiny-npm-license": "^1.0.0",
    "vinyl-source-stream": "^1.0.0"
